<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<classPathEntry
		location="D:\workspace\EclipseWorkSpace\SFServer\WebContent\WEB-INF\lib\mysql-connector-java-5.1.6-bin.jar" />

	<context id="DB2Tables" targetRuntime="MyBatis3">
		<plugin type="com.miaoyou.platform.server.dataGen.GenData" />
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/hpsf" userId="root"
			password="123456">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.miaoyou.platform.server.entity"
			targetProject="D:\workspace\EclipseWorkSpace\SFServer\src\">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.miaoyou.platform.server.mapper"
			targetProject="D:\workspace\EclipseWorkSpace\SFServer\src\">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.miaoyou.platform.server.mapper" targetProject="D:\workspace\EclipseWorkSpace\SFServer\src\">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>


		<table schema="hpsf" tableName="roletb" domainObjectName="Roletb" />
		<table schema="hpsf" tableName="logtb" domainObjectName="Logtb" />
		<table schema="hpsf" tableName="pkgenerator" domainObjectName="Pkgenerator" />
		<table schema="hpsf" tableName="appparamtb" domainObjectName="Appparamtb" />
		<table schema="hpsf" tableName="departmenttb" domainObjectName="Departmenttb" />
		<table schema="hpsf" tableName="usertb" domainObjectName="Usertb" />
		<table schema="hpsf" tableName="diagnosistb" domainObjectName="Diagnosistb" />

	</context>
</generatorConfiguration>  
